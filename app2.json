{
    "id": "/app2",
    "instances": 1,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "qwertmax/app2",
            "network": "BRIDGE",
            "forcePullImage": true,
            "portMappings": [
                {
                    "containerPort": 9092,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "healthChecks": [{
        "protocol": "TCP",
        "gracePeriodSeconds": 600,
        "intervalSeconds": 30,
        "portIndex": 0,
        "timeoutSeconds": 10,
        "maxConsecutiveFailures": 2
    }],
    "ports": [9092],
    "cpus": 0.5,
    "mem": 256.0
}
